<div class="tabs-container">
    <nav class="sheet-tabs tabs" data-group="primary">
        {{#each tabs as |tab|}}
            <div class="{{tab.cssClass}}" data-action="tab" data-group="{{tab.group}}" data-tab="{{tab.id}}">
                <div class="navsection">
                    {{localize tab.title}}
                </div>
            </div>
        {{/each}}
    </nav>
</div>